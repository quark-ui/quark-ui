{
  "name": "quark-ui",
  "version": "0.0.8",
  "description": "Quark UI, Components base on react.",
  "repository": "https://github.com/quark-ui/quark-ui.git",
  "author": "ryan.bian",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "start": "webpack-dashboard -- webpack-dev-server --config ./conf/webpack.conf.js --port 3000 --open",
    "start-nodash": "webpack-dev-server --config ./conf/webpack.conf.js --port 3000 --open",
    "theme": "webpack --config ./conf/webpack.conf.js",
    "dll": "webpack --config ./conf/webpack.dll.conf.js",
    "gh-pages": "webpack --config ./conf/webpack.conf.js",
    "pub": "git push origin master & npm publish",
    "new": "yo quarkui",
    "lint": "eslint --color -f table ./src",
    "lint-c": "eslint --color -f table",
    "build": "node ./conf/publish.build.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/setup.js --recursive ./src/components/**/*.test.js",
    "test-unit": "mocha --compilers js:babel-core/register --require ./test/setup.js --recursive",
    "test-chrome": "karma start ./conf/karma.conf.js",
    "test-all": "karma start ./conf/karma.conf.js --browers PhantomJS Chrome Firefox",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- --report lcovonly --require ./test/setup.js  ./src/components/**/*.test.js --compilers babel-core/register  -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test-nyc": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "bugs": {
    "url": "http://github.com/quark-ui/quark-ui/issues"
  },
  "keywords": [
    "react",
    "react-component",
    "quark-ui"
  ],
  "files": [
    "src",
    "lib",
    "es"
  ],
